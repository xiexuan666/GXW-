<!-- 头部 -->
<view class="jinx-t">
	<view class="jinx-tnr">
		<view class="jinx-bei">
			<image src="https://cdn.juesedao.cn/gxw/mingpian.png"></image>
		</view>
		<view class="jinx-toux">
			<view class="toux-img">
				<image src="https://cdn.juesedao.cn/gxw/18655adabbbd420588b323d2357f1242"></image>
			</view>
			<view class="name-p">冠星王总部</view>
		</view>
	</view>
</view>


<!-- 卡片 -->
<block wx:for='{{list}}' wx:index='index' wx:item='item'>
<view class="jxsnr">
	
	<view class="jxsnr-ul">
		<view class="jxsnr-ul-r">
			{{item.name}}
		</view>
		<view class="jxsnr-li">
			<view class="jxsnr-tubiao tu1">
				<image mode="heightFix" src="/images/tubiao/46.png"></image>
			</view>
			<view class="jxsnr-li-lp">	{{item.phone}}</view>
		</view>
		<view class="jxsnr-li">
			<view class="jxsnr-tubiao tu1">
				<image mode="heightFix" src="/images/tubiao/40.png"></image>
			</view>
			<view class="jxsnr-li-lp">	{{item.address}}</view>
		</view>
		<view class="jxsnr-li">
			<view class="jxsnr-tubiao tu1">
				<image mode="heightFix" src="/images/tubiao/46.png"></image>
			</view>
			<view class="jxsnr-li-lp">{{item.date}}</view>
		</view>
		<view class="keh">
			<image src="https://cdn.juesedao.cn/gxw/lianxikehu.png" bindtap="calling" catchtap="calling" data-index="{{item.phone}}"></image>
		</view>
	</view>

</view> 
</block>