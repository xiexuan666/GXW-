<!-- 产品展示 -->
<view class="produt">
	<view class="toub">
		<view class="toub-nr">
			<view class="toub-nrl">
				<view class="toub-nrlp1">
					<image mode="widthFix" src="/images/shouye/13.png" class="sheng"></image>
				</view>
			</view>
			<view class="toub-nrr" bindtap="tzcpanliye">
				<view class="toub-nrlp2">/ 更多</view>
			</view>
		</view>
	</view>
	<swiper class="lbswipers">
		<block wx:for="{{lunbs}}" wx:key="index">
			<swiper-item>
				<view class="swiper-item" data-url="{{item.photo}}" bindtap="handlelun">
					<image mode="aspectFill" src="{{item.photo}}" class="item-img"></image>
				</view>
				<view class="header">
					<view class="tatli">
						{{item.name}}
					</view>
					<view class="header-img">
						<text class="xian"># {{item.type_name}}</text>
						<text class="xi"># {{item.chengshi}}</text>
						<text class="ping"># {{item.huxing}}</text>
					</view>
				</view>
			</swiper-item>
		</block>
	</swiper>



</view>




