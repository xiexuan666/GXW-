<!-- 搜索 -->
<view class="lunb">
	<swiper class="lbswiper" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular='{{circular}}' indicator-dots="{{true}}" autoplay="{{true}}">
		<block wx:for="{{lunb}}" wx:key="index">
			<swiper-item>
				<view class="swiper-item lunbimg " data-url="{{item.link}}">
					<image mode="widthFix" lazy-load="{{true}}" src="{{item.url}}"></image>
				</view>
			</swiper-item>
		</block>
	</swiper>
</view>


<!-- 导航 -->
<view class="daoh">
	<view class="daoh-ul">
		<view class="daoh-li " bindtap="tzcase">
			<image mode="widthFix" src="/images/tubiao/1-02.png"></image>
			<view class="daoh-li-text">CASE</view>
			<view class="daoh-li-p">看案例</view>
		</view>
		<view class="daoh-li " bindtap="tzproduct">
			<image mode="widthFix" src="/images/tubiao/1-03.png"></image>
			<view class="daoh-li-text">PRODUCT</view>
			<view class="daoh-li-p">找产品</view>
		</view>
		<view class="daoh-li " bindtap="tzstrategy">
			<image mode="widthFix" src="/images/tubiao/1-04.png"></image>
			<view class="daoh-li-text">STRATEGY</view>
			<view class="daoh-li-p">做攻略</view>
		</view>
		<view class="daoh-li " bindtap="tzbrand">
			<image mode="widthFix" src="/images/tubiao/1-05.png"></image>
			<view class="daoh-li-text">BRAND</view>
			<view class="daoh-li-p">逛品牌</view>
		</view>
		<view class="daoh-li " bindtap="tzroom">
			<image mode="widthFix" src="/images/tubiao/1-06.png"></image>
			<view class="daoh-li-text">RESERVATION</view>
			<view class="daoh-li-p">约量房</view>
		</view>
	</view>
</view>



