<!-- 搜索 -->
<view class="lunb">
	<swiper class="lbswiper" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular='{{circular}}'>
		<block wx:for="{{lunb}}" wx:key="index">
			<swiper-item>
				<view class="swiper-item lunbimg " data-url="{{item.link}}">
					<image mode="widthFix" lazy-load="{{true}}" src="{{item.url}}"></image>
				</view>
			</swiper-item>
		</block>
	</swiper>
</view>
