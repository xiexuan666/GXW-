  box-shadow: 0 0 15rpx 0rpx #ccc;
}
.anlilist-liimg{
  width: 100%;
  font-size: 0rpx;
  height: 400rpx;
  overflow: hidden;
  
}
.anlilist-liimg image{
  width: 100%;
  height: 100%;
  
}
.anlilist-lib{
  background-color:#F7F7F7;
  
  
}
.anlilist-libp1{
  color: #716F6F;
