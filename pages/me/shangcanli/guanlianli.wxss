.guananli-t{
  width: 680rpx;
  margin-bottom: 0 auto;
}
.guananli-tp{
  color: #716F6F;
  text-align: right;
  font-size: 32rpx;
  font-weight: 300;
  margin-top: 30rpx;
  margin-bottom: 35rpx;
}
.anlilist-lis1{
  width: 625rpx;
  margin-left: 64rpx;
  position: relative;
}
.anlilist-lis1::after{
  content: '';
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  border: 2px solid #716F6F;
  left: -65rpx;
  top: 40%;
}
.anlilist-lis2{
  width: 625rpx;
  margin-left: 64rpx;
  position: relative;
  box-shadow: 0 0 15rpx 0rpx #ccc;
}
.anlilist-lis2::after{
  content: '';
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  border: 2px solid #716F6F;
  left: -65rpx;
  top: 49%;
  background-color: #716F6F;
  
}

